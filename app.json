{
  "expo": {
    "name": "Umaps",
    "slug": "Umaps",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/umass_event_maps_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/umass_event_maps_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.phoenixfi.umaps",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to display it on the map."
      }
      
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/umass_event_maps_logo_adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.phoenixfi.umaps"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "./plugins/android-manifest.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c1e7dadb-4359-41d9-9787-20fc4dd951ed"
      }
    },
    "owner": "umaps"
  }
}